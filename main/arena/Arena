package main.arena;

import java.util.ArrayList;
import java.util.List;

import org.bukkit.Location;

public class Arena {

	String name;	
	Location p1;
	Location p2;
    Location lobby;
	Location spawn;
	List<String> players = new ArrayList<String>();
	
	public Arena(String name, Location p1, Location p2, Location lobby, Location spawn) {
		this.name = name;
		this.p1 = p1;
		this.p2 = p2;
		this.lobby = lobby;
		this.spawn = spawn;	
	}
	
	public String getName() {
		return this.name;
	}
	
	public List<String> getPlayers(){
        return this.players;
	}
}
